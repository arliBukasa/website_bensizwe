<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="snippets" inherit_id="website.snippets" name="Rows_jobs Snippets">
            <xpath expr="//*[@id='default_snippets']" position="before">
                <t id="x_theme_snippets">
                    <div id="x_theme_snippets_category" class="o_panel">
                        <div class="o_panel_header">Arnold's snippets</div>
                        <div class="o_panel_body">
                            <t t-snippet="website_bensizwe.snippet_jobs_rows" t-thumbnail="/website_bensizwe/static/src/img/jobs.svg">
                                <keywords>Snippet</keywords>
                            </t>
                             <t t-snippet="website_bensizwe.snippet_jobs_out_rows" t-thumbnail="/website_bensizwe/static/src/img/jobs.svg">
                                <keywords>Snippet</keywords>
                            </t>
                           <!-- ajout du snippet trannig -->
                            <t t-snippet="website_bensizwe.dynamic_training_snippet" t-thumbnail="/website_bensizwe/static/src/img/jobs.svg">
                                <keywords>Snippet</keywords>
                            </t>
                            <t t-snippet="website_bensizwe.dynamic_job_filter_snippet" t-thumbnail="/website_bensizwe/static/src/img/jobs.svg">
                                <keywords>Snippet</keywords>
                            </t>
                        </div>
                    </div>
                </t>
            </xpath>
        </template>

    </data>
</odoo>